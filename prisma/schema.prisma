generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Producto {
  id   Int     @id @default(autoincrement())
  name String
  code String
  price Int
}

model User {
  id   Int     @id @default(autoincrement())
  username String
  email String
}